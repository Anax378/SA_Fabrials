{
    "multipart": [
        {   "apply": { "model": "sa_fabrials:block/stormlight_pipe_core" }},
        {   "when": { "connected_north": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part", "uvlock": false }
        },
        {   "when": { "connected_east": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part", "y": 90, "uvlock": false }
        },
        {   "when": { "connected_south": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part", "y": 180, "uvlock": false }
        },
        {   "when": { "connected_west": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part", "y": 270, "uvlock": false }
        },
        {   "when": { "connected_up": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part", "x": 270, "uvlock": false }
        },
        {   "when": { "connected_down": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part", "x": 90, "uvlock": false }
        }
    ]
}