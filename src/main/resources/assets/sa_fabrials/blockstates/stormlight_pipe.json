{
    "multipart": [
        {   "apply": { "model": "sa_fabrials:block/stormlight_pipe_core" }},



        {   "when": { "connected_north": "true", "is_input_north":  "true"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_in", "uvlock": false }
        },
        {   "when": { "connected_north": "true", "is_input_north":  "false"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_out", "uvlock": false }
        },



        {   "when": { "connected_east": "true" , "is_input_east":  "true"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_in", "y": 90, "uvlock": false }
        },
        {   "when": { "connected_east": "true", "is_input_east": "false"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_out", "y": 90, "uvlock": false }
        },



        {   "when": { "connected_south": "true", "is_input_south":  "true"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_in", "y": 180, "uvlock": false }
        },
        {   "when": { "connected_south": "true", "is_input_south":  "false"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_out", "y": 180, "uvlock": false }
        },



        {   "when": { "connected_west": "true", "is_input_west": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_in", "y": 270, "uvlock": false }
        },
        {   "when": { "connected_west": "true", "is_input_west":  "false" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_out", "y": 270, "uvlock": false }
        },




        {   "when": { "connected_up": "true", "is_input_up": "true" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_in", "x": 270, "uvlock": false }
        },
        {   "when": { "connected_up": "true", "is_input_up": "false" },
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_out", "x": 270, "uvlock": false }
        },




        {   "when": { "connected_down": "true", "is_input_down":  "true"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_in", "x": 90, "uvlock": false }
        },
        {   "when": { "connected_down": "true", "is_input_down":  "false"},
            "apply": { "model": "sa_fabrials:block/stormlight_pipe_part_out", "x": 90, "uvlock": false }
        }
    ]
}